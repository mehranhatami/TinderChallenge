<script>

$( document ).ready(function() {



  <% for(var i=1; i<tinder.length+1; ++i) { %>


    $('<div class="card"></div>').appendTo('#cardcontain');
    $('<div id="profile"></div>').appendTo('.class');
    var img = $('<img id="dynamic">');
    img.attr('src', <%= tinder.imgURL[i] %>);
    img.appendTo('#profile');
  <% } %>


  $(".card").on("swiperight",function(){


    $(this).addClass('rotate-left').delay(500).fadeOut("fast");

    $('.card').find('.status').remove();

    $(this).append('<div class="status like">Like!</div>');

    if ( $(this).is(':last-child') ) {


      // Get the first child again, and remove the rotate classes before fading in
      $('.card:nth-child(1)').removeClass ('rotate-left rotate-right').fadeIn(300);

     } else {

       // If this is not last child, you get the next child and remove the
       // rotate classes before fading in
        $(this).next().removeClass('rotate-left rotate-right').fadeIn(400);

     }

  });







});

</script>
